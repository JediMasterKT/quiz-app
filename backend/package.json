{
  "name": "quiz-app-backend",
  "version": "1.0.0",
  "description": "Backend for Quiz App",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:tdd": "node test-tdd.js",
    "test:progression": "node test-progression-suite.js",
    "test:integration": "node test-integration.js",
    "test:all": "npm run test:tdd && npm run test:progression",
    "verify": "node verify-demo.js",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo:all",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "db:reset": "npm run migrate:undo && npm run migrate && npm run seed"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.1.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "redis": "^5.6.0",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.0.0"
  },
  "keywords": [
    "quiz",
    "game",
    "api",
    "express"
  ],
  "author": "Quiz App Team",
  "license": "ISC"
}
